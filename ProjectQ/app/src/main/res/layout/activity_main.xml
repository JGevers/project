<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.teamawesome.projectq.MainActivity">


    <Button
        android:id="@+id/testBtn"
        android:layout_width="89dp"
        android:layout_height="48dp"
        android:text="Test"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/seBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.074" />

    <Button
        android:id="@+id/businessBtn"
        android:layout_width="146dp"
        android:layout_height="54dp"
        android:text="Business"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="45dp"
        app:layout_constraintTop_toBottomOf="@+id/ictBtn" />

    <Button
        android:id="@+id/itsmBtn"
        android:layout_width="151dp"
        android:layout_height="50dp"
        android:text="ITSM"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="51dp"
        app:layout_constraintTop_toBottomOf="@+id/businessBtn" />

    <Button
        android:id="@+id/seBtn"
        android:layout_width="155dp"
        android:layout_height="53dp"
        android:text="Software Engineering"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="57dp"
        app:layout_constraintTop_toBottomOf="@+id/itsmBtn" />

    <Button
        android:id="@+id/ictBtn"
        android:layout_width="150dp"
        android:layout_height="61dp"
        android:text="HBO ICT"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="73dp" />

</android.support.constraint.ConstraintLayout>
